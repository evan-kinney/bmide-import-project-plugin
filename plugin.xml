<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.8"?>
<!DOCTYPE xml>
<plugin>
   <extension point="org.eclipse.ui.startup">
      <startup class="com.evandashkinney.bmide.ImportProjects"/>
   </extension>
   <extension
       id="headlessimport"
       name="Headless Import"
       point="org.eclipse.core.runtime.applications">
       <application
           cardinality="singleton-global"
           thread="main"
           visible="true">
           <run
               class="com.evandashkinney.bmide.HeadlessImportApplication">
           </run>
       </application>
   </extension>
</plugin>
